    <description>placebo</description>
  </arm_group>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Dextroamphetamine</intervention_name>
    <description>15 mg for first 8 weeks and 30 mg for 2nd 8 weeks</description>
    <arm_group_label>1</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>D-Amphetamine</intervention_name>
    <description>30 mg for the first 8 weeks and 60 for the second 8 weeks (16 weeks total)</description>
    <arm_group_label>2</arm_group_label>
  </intervention>
  <intervention>
    <intervention_type>Drug</intervention_type>
    <intervention_name>Placebo</intervention_name>
