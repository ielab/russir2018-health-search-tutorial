  <brief_title>Dextroamphetamine as an Adjunct in Cocaine Treatment - 1</brief_title>
  <official_title>Dextroamphetamine as an Adjunct in Cocaine Treatment</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>National Institute on Drug Abuse (NIDA)</agency>
      <agency_class>NIH</agency_class>
    </lead_sponsor>
